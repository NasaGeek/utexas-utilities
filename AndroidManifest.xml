<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.nasageek.UTilities"
      android:versionCode="6"
      android:versionName="1.05">
    <uses-sdk android:minSdkVersion="7"
    		  android:targetSdkVersion="15"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    
    <application android:icon="@drawable/launcher" android:label="@string/app_name">
        <activity android:name=".UTilitiesActivity"
        			
                  android:label="@string/app_name"
                  android:clearTaskOnLaunch="true"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity android:name=".DataUsageActivity"
         			android:screenOrientation="portrait"></activity>
        <activity android:name=".AboutMeActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
       
        <activity android:name=".Preferences"></activity>
        <activity android:name=".ScheduleActivity"
        
        	android:screenOrientation="portrait"></activity>
        <activity android:name=".CampusMapActivity"
        			android:launchMode="singleTop"
        			>
        	<intent-filter>
	            <action android:name="android.intent.action.SEARCH" />
	        </intent-filter>
        	<meta-data android:name="android.app.searchable"
					   android:resource="@xml/searchable"/>
        		</activity>
        <activity android:name=".BalanceActivity" 

        			android:screenOrientation="portrait"
        		></activity>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".ExamScheduleActivity"
        			android:screenOrientation="portrait"></activity>
        
        	
        <provider android:name=".BuildingProvider"
         			 android:authorities="com.nasageek.utilities.buildingprovider" 
         />
        <uses-library android:name="com.google.android.maps" />
        

    </application>
</manifest>